{
    "name": "flarumes/lang-spanish",
    "type": "flarum-extension",
    "homepage": "https://github.com/FlarumES/lang-spanish",
    "description": "Paquete de idioma español para Flarum y sus extensiones",
    "keywords": ["locale"],
    "license": "MIT",
    "authors": [
        {
            "name": "FlarumES",
            "email": "FlarumES@gmail.com",
            "homepage": "https://FlarumES.com"
        }
    ],
    "support": {
        "issues": "https://github.com/FlarumES/lang-spanish/issues",
        "source": "https://github.com/FlarumES/lang-spanish"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.16"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
    },
        "flarum-extension": {
            "title": "Español",
            "category": "language",
            "info": {
                "website": "https://FlarumES.com",
                "support": "https://github.com/FlarumES/lang-spanish/issues"
            },
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#fff",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "es",
            "title": "Español"
        }
    }
}